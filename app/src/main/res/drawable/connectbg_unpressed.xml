<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white2" />
        </shape>
    </item>

    <!--bottom-->
    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape>
            <stroke
                android:width="2dp"
                android:color="#d2d9e2" />
        </shape>
    </item>

    <!--top-->
    <item
        android:bottom="-2dp"
        android:left="-2dp"
        android:right="-2dp">
        <shape>
            <stroke
                android:width="2dp"
                android:color="#d2d9e2" />
        </shape>
    </item>


    <!--right-->
    <item
        android:bottom="-1dp"
        android:left="-1dp"
        android:top="-1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#e1e6ec" />
        </shape>
    </item>


</layer-list>